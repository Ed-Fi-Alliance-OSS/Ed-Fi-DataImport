@using DataImport.Web.Infrastructure
@model DataImport.Web.Features.Agent.UploadFile.Command

@{
    ViewBag.Title = "Upload File";
}

<h2>@ViewBag.Title</h2>

<h4>Manual Upload</h4>

@using (Html.Form())
{
    @Html.HiddenFor(m => m.AgentId)
    @Html.Input(x => x.File)
    @Html.SubmitButton("Upload")
}
